		<VideoPlayer name="vplayer">
			<Size>
				<AbsDimension x="1280" y="1024" />
			</Size>
			<!-- <fms>rtmp://192.168.100.76/vod</fms> -->
			<flv>file://C:/partage/300.flv</flv>
		</VideoPlayer>
		
		<!-- Texture txt_habillage-->

		<Texture name="txt_habillage">
			<Size>
				<AbsDimension x="1280" y="1024" />
			</Size>
			<uri>[local.assets]Video_tx_habillage</uri>
			<autoGrid>false</autoGrid>
		</Texture>
		
		<!-- Texture tx_loading_bg-->

		<Texture name="tx_loading_bg">
			<Anchors>
